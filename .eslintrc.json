{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "quotes": "off",
    // "no-unused-vars": "off",
    // "no-undef": "off",
    "no-plusplus": "off",
    // "no-nested-ternary": "off",
    "no-use-before-define": "off"
    // "no-param-reassign": "off",
    // "no-shadow": "off",
    // "no-inner-declarations": "off",
    // "no-loop-func": "off"
  }
}
